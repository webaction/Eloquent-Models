{
    "name": "faustbrian/eloquent-models",
    "description": "Adds basic models for Laravel",
    "keywords": ["laravel", "framework", "Eloquent-Models", "Eloquent Models", "Brian Faust", "faustbrian"],
    "license": "MIT",
    "authors": [{
        "name": "Brian Faust",
        "email": "hello@brianfaust.me",
        "homepage": "https://brianfaust.me"
    }],
    "require": {
        "php": "^7.0",
        "illuminate/support": "5.3.* || 5.4.* || 5.5.*",
        "illuminate/database": "5.3.* || 5.4.* || 5.5.*",
        "nesbot/carbon": ">=1.21.0",
        "faustbrian/eloquent-schema": "^1.0"
    },
    "require-dev": {
        "graham-campbell/testbench": "^3.3",
        "mockery/mockery": "^0.9.5",
        "phpunit/phpunit": "^6.1"
    },
    "autoload": {
        "psr-4": {
            "BrianFaust\\Models\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "BrianFaust\\Tests\\Models\\": "tests"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
